@article{Johnson07,
author = {Johnson, John Asher and Butler, R Paul and Marcy, Geoffrey W and Fischer, Debra A and Vogt, Steven S and Wright, Jason T and Peek, Kathryn M G},
title = {{A New Planet around an M Dwarf: Revealing a Correlation between Exoplanets and Stellar Mass}},
