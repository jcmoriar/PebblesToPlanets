@article{West07,
author = {West, Jennifer L. and English, Jayanne and Normandeau, Magdalen and Landecker, T. L.},
title = {{The Fragmenting Superbubble Associated with the H II Region W4}},
journal = {ApJ},
year = {2007},
volume = {656},
number = {2},
pages = {914--927},
month = feb,
affiliation = {Department of Physics and Astronomy, University of Manitoba, Winnipeg, MB, Canada english@physics.umanitoba.ca jennifer_west@umanitoba.ca},
doi = {10.1086/510609},
rating = {0},
date-added = {2012-01-30T16:11:23GMT},
date-modified = {2013-01-28T14:50:51GMT},
abstract = {New observations at high latitudes above the H II region W4 show that the structure formerly identified as a chimney candidate, an opening to the Galactic halo, is instead a superbubble in the process of fragmenting and possibly evolving into a chimney. Data at high Galactic latitudes (b>5deg) above the W3/W4 star-forming region at 1420 and 408 MHz Stokes I (total power) and 1420 MHz Stokes Q and U (linear polarization) reveal an egg-shaped structure with morphological correlations between our data and the H$\alpha$ data of Dennison, Topasna, and Simonetti. Polarized intensity images show depolarization extending from W4 up the walls of the superbubble, providing strong evidence that the radio continuum is generated by thermal emission coincident with the H$\alpha$ emission regions. We conclude that the parts of the H II region hitherto known as W4 and the newly revealed thermal emission are all ionized by the open cluster OCl 352. At an assumed distance of 2.35 kpc, the ovoid structure is 164 pc wide and extends 246 pc above the midplane of the Galaxy. The shell's emission decreases in total intensity and polarized intensity in various locations, appearing to have a break at its top and another on one side. Using a geometric analysis of the depolarization in the shell's walls, we estimate that a magnetic field line-of-sight component of 3-5 $\mu$G exists in the shell. We explore the connection between W4 and the Galactic halo, considering whether sufficient radiation can escape from the fragmenting superbubble to ionize the kiloparsec-scale H$\alpha$ loop discovered by Reynolds, Sterling and Haffner.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2007ApJ...656..914W&link_type=ABSTRACT},
local-url = {file://localhost/Users/johncmoriarty/Library/Application%20Support/Papers2/Articles/2007/West/ApJ%202007%20West.pdf},
file = {{ApJ 2007 West.pdf:/Users/johncmoriarty/Library/Application Support/Papers2/Articles/2007/West/ApJ 2007 West.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/510609}}
}

