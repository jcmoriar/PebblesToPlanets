file = {{ApJ 2011.pdf:/Users/johncmoriarty/Library/Application Support/Papers2/Articles/2011/Unknown/ApJ 2011.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/736/2/85}}
