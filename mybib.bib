@article{2014ApJS..210....5F,
author = {Fischer, Debra A and Marcy, Geoffrey W and Spronck, Julien F P},
title = {{The Twenty-five Year Lick Planet Search}},
journal = {The Astrophysical Journal Supplement},
year = {2014},
volume = {210},
number = {1},
pages = {5},
month = jan,
affiliation = {Department of Astronomy, Yale University, New Haven, CT 06520, USA debra.fischer@yale.edu},
